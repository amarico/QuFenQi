body
{
    color: #555;
    font-size: 12px;
    font-family: 'Microsoft YaHei';
    letter-spacing: 2px;
    padding: 0px;
    margin: 0px;
    width: 100%;
    height: 100%;
}

h1, h2, h3
{
    font-weight: normal;
    margin-top: 0px;
    padding-top: 0px;
}



.header
{
    width: 100%;
    min-height:57px;
    max-height: 67px;
    display: table-cell;
    display: table;
    padding-bottom:12px;
}

    .header img
    {
        min-height:36px;
        max-height:77px;
        display: table-cell;
        vertical-align: middle;
    }

.content
{
    width: 100%;
    height: 509px;
    margin-top: 0px;
    /*border-top:1px inset #ddd;
    border-bottom:1px inset #ddd;*/
}

.tool
{
    position: relative;
    width: 100%;
    text-align: center;
    color: #a5a5a5;
    margin-top: 25px;
}

#left
{
    width: 308px;
    height: 507px;
    float: left;
    border: 1px solid #ddd;
    padding-left: 35px;
    text-align: center;
    background-color: rgba(240, 240, 240, 0.15);
}


#right
{
    width: 234px;
    height: 104px;
    float: left;
    padding-left: 37px;
    padding-top: 22px;
    background-image: url(../images/boeder.png);
    background-repeat: no-repeat;
}

#divWrongMessage
{
    width: 234px;
    height: 249px;
    float: left;
    padding-left: 37px;
    padding-top: 18px;
    background-image: url(../images/boeder222.png);
    background-repeat: no-repeat;
    color: #f00;
    display: none;
}

    #divWrongMessage p
    {
        margin: 0px;
        padding: 0px;
    }

 h4
{
    margin: 0px;    
    margin-bottom:5px;
    color:#f00;
}

.main
{
    position: absolute;
    left: 361px;
    top: 16px;
    width: 616px;
}




a:visited, a:link
{
    color: #555;
    text-decoration: none;
}

a:hover
{
    color: #f54343;
    text-decoration: underline;
}



.tool a:visited, .tool a:link
{
    color: #a5a5a5;
    text-decoration: none;
}

.tool a:hover
{
    color: #f54343;
    text-decoration: underline;
}





table tr
{
    height: 42px;
}

table tr th
{
    padding-top:8px;
}


    table tr td
    {
        padding-bottom: 11px;
    }

        table tr td .tetx
        {
            width: 174px;
        }




    table tr.trHead
    {
        font-size: 16px;
        font-weight: bold;
        padding-top: 25px;
        height: 45px;
    }

    table tr.trSpace
    {
        height: 2px;
    }

.decory
{
    text-align: right;
}

td input
{
    height: 100%;
    width: 100%;
    border: 1px solid #ddd;
}





#inputPictureRegist
{
    height: 26px;
    width: 99px;
    float: left;
}

#picture
{
    height: 30px;
    width: 75px;
    background-image: url("../images/cccc.png");
    float: left;
    font-size: 16px;
    padding-top: 5px;
    padding-left: 6px;
    letter-spacing: 4px;
    color: #fff;
    cursor: pointer;
    text-align: center;
}








input[type=checkbox]
{
    -webkit-appearance: none;
    width: 13px;
    height: 13px;
    vertical-align: middle;
    background: #fff;
    border: 1px solid #B9BBBE;
    -webkit-border-radius: 1px;
    -moz-border-radius: 1px;
    border-radius: 1px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    position: relative;
    cursor: pointer;
}


    input[type=checkbox]:hover
    {
        border-color: #c6c6c6;
        -webkit-box-shadow: none;
        -moz-box-shadow: none;
        box-shadow: none;
    }

    input[type=checkbox]:checked
    {
        background-image: url("../images/checkBackground.png");
    }




    input[type=checkbox]:focus
    {
        outline: none;
        -webkit-box-shadow: none;
        -moz-box-shadow: none;
        box-shadow: none;
    }

table
{
    padding: 0px;
    margin: 0px;
    border-collapse: collapse;
}

.pTitle
{
    font-size: 16px;
